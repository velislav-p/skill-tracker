@model DevEnv_Semester_Project.Models.UserSkillLevelsViewModel


    <div class="list-group">
    @foreach (var item in Model.UserSkillLevels)
    {
    <div class="list-group-item list-group-item-action flex-column align-items-start">
        <div style="display:flex; justify-content:space-between;" class="d-flex w-100 justify-content-between">
            <span class="h4 mb-1">@item.Skill.Name</span>
            <small>@item.SkillLevel.SkillMastery</small>
        </div>
        
    </div>  
    }
</div>
